#include "/Engine/Private/Common.ush"
#include "/Engine/Private/ScreenPass.ush"

Texture2D InputTexture;
SamplerState InputTextureSampler;

float4 MainPS(FScreenVertexOutput In) : SV_Target0
{
    // 因为 OutputViewport 和 InputViewport 相同, 尽管 RenderTarget 不同, UE 内部会自动处理, 
    // 这里只需原样用 dest UV 当作 source UV 采样即可. 
    return InputTexture.SampleLevel(InputTextureSampler, In.UV, 0.0f);
}
